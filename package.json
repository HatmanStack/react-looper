{
  "name": "migration",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "build:web:analyze": "ANALYZE=true expo export:web",
    "serve:web": "npx serve web-build -l 8081",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck && npm run test",
    "eas:build:dev": "eas build --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:prod": "eas build --profile production",
    "eas:build:android": "eas build --platform android --profile production",
    "eas:build:ios": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.4",
    "@ffmpeg/ffmpeg": "^0.12.6",
    "@ffmpeg/util": "^0.12.2",
    "@react-native-community/slider": "^5.1.1",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "dayjs": "^1.11.19",
    "expo": "~54.0.23",
    "expo-av": "^16.0.7",
    "expo-dev-client": "^6.0.17",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.17",
    "expo-linking": "^8.0.8",
    "expo-media-library": "^18.2.0",
    "expo-router": "^6.0.14",
    "expo-status-bar": "~3.0.8",
    "ffmpeg-kit-react-native": "^6.0.2",
    "lamejs": "^1.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@react-native/babel-preset": "^0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
